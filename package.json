{
  "name": "benrombautca",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "echo  Linting Disabled!!!!!!!  # vue-cli-service lint",
    "prepare": "husky install",
    "sync-bookshelf": "bash ./scripts/sync_bookshelf.sh",
    "sync-software": "bash ./scripts/sync_software.sh"
  },
  "dependencies": {
    "@brombaut/types": "1.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.18.2",
    "dotenv": "^10.0.0",
    "firebase-firestore-facade": "1.2.0",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "5.x.x",
    "@typescript-eslint/parser": "5.x.x",
    "@vue/cli-plugin-babel": "5.x.x",
    "@vue/cli-plugin-typescript": "5.x.x",
    "@vue/cli-service": "5.x.x",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "sass": "^1.35.1",
    "sass-loader": "^8.0.2",
    "typescript": "^4.9.5",
    "vue-template-compiler": "^2.7.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "ignorePatterns": [
      "f3_syncer.ts",
      "f3_syncer.js"
    ],
    "rules": {
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "no-shadow": 0,
      "no-param-reassign": 0,
      "arrow-parens": 0,
      "arrow-body-style": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "func-names": 0,
      "prefer-arrow-callback": 0,
      "space-before-function-paren": 0,
      "no-use-before-define": 0,
      "no-plusplus": 0,
      "no-confusing-arrow": 0,
      "class-methods-use-this": 0,
      "no-underscore-dangle": 0,
      "padded-blocks": 0,
      "lines-between-class-members": 0,
      "object-curly-newline": 0,
      "no-await-in-loop": 0,
      "import/no-cycle": 0,
      "max-classes-per-file": 0,
      "no-useless-constructor": 0,
      "no-empty-function": 0,
      "quotes": [
        "error",
        "double"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "max-len": [
        "warn",
        600
      ],
      "indent": [
        "error",
        2
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
